(load "senva/derxi.snv")

(define check
  (lambda (code succ)
	(do (prin "CHECK: \"" code "\" -> ")
	  (! let ((result (senva::eval (senva::readtop code) (environment))))
	   (do (print result)
		 (if (equal result succ) nil (throw 44 "Fail!")))))))

(define echeck
  (lambda (code ceid succ)
	(do (prin "ECHECK: \"" code "\" -> ")
	  (catch (lambda (eid message)
				 (do (print "<Erro \"" message "\">")
				   (if (and (equal ceid eid) (equal message succ))
					 nil
					 (throw 444 (sprint "expect code: " ceid
										", but got code: " eid)))))
		   (do (print (senva::eval (senva::readtop code) (environment)))
			   (throw 4444 "NEFail!"))))))

(define vcheck
  (lambda (code succ)
	(do (prin "VCHECK: " code " -> ")
	  (! let ((result ((derxi::vm code) 'run)))
	   (do (print result)
		 (if (equal result succ) nil (throw 44 "Fail!")))))))

(define evcheck
  (lambda (code ceid succ)
	(catch (lambda (eid message)
			 (do (print "EVCHECK: " code " -> <Erro \"" message "\">")
			   (if (and (equal ceid eid) (equal message succ))
				 nil
				 (throw 444 "EFail!"))))
		   (do (! let ((result ((derxi::vm code) 'run)))
				  (print "EVCHECK: " code " -> " result))
			 (throw 4444 "NEFail!")))))


