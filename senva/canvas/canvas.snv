(load "senva/util.snv")

(define canvas ((-> (js "document") "getElementById") "main-canvas"))
(define context ((-> canvas "getContext") "2d"))
((-> context "fillRect") 20 20 300 200)
(define img (new "Image"))
(setat img "src" "perl.png")
((-> context "drawImage") img 0 0)

